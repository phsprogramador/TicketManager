{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database={{ secrets.APP_DATABASE }};User Id={{ secrets.APP_DATABASE_USER }};Password={{ secrets.APP_DATABASE_PASS }};TrustServerCertificate=True"
  }
}